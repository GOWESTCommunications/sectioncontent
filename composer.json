{
    "name": "gowest/sectioncontent",
    "type": "typo3-cms-extension",
    "description": "Create page teasers with data from page properties and its content elements.",
    "version": "13.0.2",
    "homepage": "https://go-west.at",
    "license": ["GPL-2.0-or-later"],
    "keywords": ["TYPO3 CMS"],
    "require": {
        "typo3/cms-core": "^13.4",
        "friendsoftypo3/headless": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "GOWEST\\Sectioncontent\\": "Classes/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "GOWEST\\Sectioncontent\\": "Classes/"
      }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "sectioncontent"
        }
    },
    "replace": {
        "sectioncontent": "self.version",
        "typo3-ter/sectioncontent": "self.version"
    }
}
